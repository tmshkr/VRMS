{
  "name": "vrms",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "start": "turbo run start --parallel",
    "start:slackbot": "npm run prisma:migrate && npm run prisma:seed && npm run start --workspace=slackbot",
    "start:web": "npm run start --workspace=web"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.6",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "18.0.5",
    "dotenv": "^16.0.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.19",
    "prettier": "latest",
    "prisma": "^4.2.1",
    "prisma-erd-generator": "^1.1.0",
    "turbo": "latest",
    "typescript": "^4.6.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@graphql-yoga/node": "^2.13.11",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^4.2.1",
    "@slack/bolt": "^3.10.0",
    "@svgr/webpack": "^6.3.1",
    "agenda": "^4.2.1",
    "axios": "^0.27.2",
    "cookies": "^0.8.0",
    "dayjs": "^1.11.5",
    "googleapis": "^104.0.0",
    "graphql": "^16.5.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "line-reader": "^0.4.0",
    "mongodb": "^4.9.0",
    "next-auth": "^4.10.3",
    "octokit": "^2.0.7",
    "rrule": "^2.6.4",
    "ts-node": "^10.7.0"
  },
  "packageManager": "npm@8.5.0",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
