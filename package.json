{
  "name": "meetbot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "start": "npm run prisma:migrate && npm run prisma:seed && turbo run start --parallel"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.6",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "18.0.5",
    "dotenv": "^16.0.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.19",
    "prettier": "latest",
    "prisma": "^4.2.1",
    "prisma-erd-generator": "^1.1.0",
    "turbo": "latest",
    "typescript": "^4.6.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.168.0",
    "@graphql-yoga/node": "^2.13.11",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.8",
    "@prisma/client": "^4.2.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@slack/bolt": "^3.10.0",
    "@svgr/webpack": "^6.3.1",
    "@tailwindcss/forms": "^0.5.3",
    "agenda": "^4.2.1",
    "aws-sdk": "^2.1213.0",
    "axios": "^0.27.2",
    "babel-plugin-superjson-next": "^0.4.4",
    "classnames": "^2.3.1",
    "cookies": "^0.8.0",
    "cuid": "^2.1.8",
    "dayjs": "^1.11.5",
    "easymde": "^2.17.0",
    "express": "^4.18.1",
    "googleapis": "^104.0.0",
    "graphql": "^16.5.0",
    "http-proxy-middleware": "^2.0.6",
    "joi": "^17.6.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "line-reader": "^0.4.0",
    "marked-react": "^1.3.0",
    "mongodb": "^4.9.0",
    "next-auth": "4.12.0",
    "next-connect": "^1.0.0-next.3",
    "next-transpile-modules": "^9.0.0",
    "octokit": "^2.0.7",
    "react-confetti": "^6.1.0",
    "react-hook-form": "^7.35.0",
    "react-redux": "^8.0.2",
    "rrule": "^2.6.4",
    "slugify": "^1.6.5",
    "superjson": "^1.10.0",
    "ts-node": "^10.7.0"
  },
  "packageManager": "npm@8.5.0",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
